{
    "version": "1.0.0",
    "description": "A port of HtmlAgilityPack to .NET Core",
    "packOptions": {
        "owners": [ "Source Tree Solutions" ],
        "tags": [ "HTML", "parse", "netcore" ],
        "projectUrl": "https://github.com/joeaudette/cloudscribe.HtmlAgilityPak",
        "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
        "copyright": "2015 Source Tree Solutions",
        "iconUrl": "https://raw.githubusercontent.com/joeaudette/cloudscribe/master/cloudscribe-icon-32.png",
        "requireLicenseAcceptance": false,
        "releaseNotes": ""
    },

    "dependencies": {
        "NETStandard.Library": "1.6.0-*"
        
    },

    "frameworks": {
        "net46": {
            "frameworkAssemblies": {
                "System.Xml": "4.0.0.0",
                "System.Xml.Linq": "4.0.0.0"
            }
        },
        "net452": {
            "frameworkAssemblies": {
                "System.Xml": "4.0.0.0",
                "System.Xml.Linq": "4.0.0.0"
            }
        },
        "net451": {
            "frameworkAssemblies": {
                "System.Xml": "4.0.0.0",
                "System.Xml.Linq": "4.0.0.0"
            }
        },
        "netstandard1.6": {
            "imports": "dnxcore50",
            "dependencies": {
                "System.Diagnostics.TraceSource": "4.0.0-*",
                "System.Xml.XmlDocument": "4.0.1-*",
                "System.Xml.XPath": "4.0.1-*",
                "System.Xml.XPath.XmlDocument": "4.0.1-*"
            }
        }
    }
}
